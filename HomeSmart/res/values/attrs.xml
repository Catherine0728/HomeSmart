<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="fill" format="boolean" />
    <attr name="Paint_Width" format="integer" />
    <attr name="Paint_Color" format="integer" />
    <attr name="Inside_Interval" format="integer" />
    <attr name="Paint_BG_Color" format="integer" />
    <attr name="value_text_color" format="integer" />
    <attr name="text_size" format="integer" />
    <attr name="value_text_size" format="integer" />
    <attr name="count" format="integer" />

    <declare-styleable name="LockPatternView">
        <attr name="aspect" format="string" />
    </declare-styleable>
    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer"/>
        <attr name="textIsDisplayable" format="boolean" />
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>

</resources>